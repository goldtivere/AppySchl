<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.GOTIT</groupId>
    <artifactId>SchoolApp</artifactId>
    <version>1.0-SNAPSHOT</version>
    <packaging>war</packaging>

    <name>SchoolApp</name>

    <properties>
        <endorsed.dir>${project.build.directory}/endorsed</endorsed.dir>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>
    
    <dependencies>
        <dependency>
            <groupId>org.primefaces</groupId>
            <artifactId>primefaces</artifactId>
            <version>5.0</version>
        </dependency>
        <dependency>
            <groupId>Lib</groupId>
            <artifactId>LinArt</artifactId>
            <version>1.0</version>
        </dependency>
        <dependency>
            <groupId>sec</groupId>
            <artifactId>secArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>third</groupId>
            <artifactId>thirdArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>fourth</groupId>
            <artifactId>fourthArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>fifth</groupId>
            <artifactId>fifthArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>six</groupId>
            <artifactId>sixArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>seven</groupId>
            <artifactId>sevenArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>eight</groupId>
            <artifactId>eightArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>ninth</groupId>
            <artifactId>ninthArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>ten</groupId>
            <artifactId>tenArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>eleven</groupId>
            <artifactId>elevenArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>twelve</groupId>
            <artifactId>twelveArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>thirteen</groupId>
            <artifactId>thirteenArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>fourteen</groupId>
            <artifactId>fourteenArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>fifteen</groupId>
            <artifactId>fifteenArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>sixteen</groupId>
            <artifactId>sixteenArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>seventeen</groupId>
            <artifactId>seventeenArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>eighteen</groupId>
            <artifactId>eighteenArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>nineteen</groupId>
            <artifactId>nineteen</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>twenty</groupId>
            <artifactId>twentyArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>twentyone</groupId>
            <artifactId>twentyoneArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>twentyTwo</groupId>
            <artifactId>twentyTwoArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>TwentyThree</groupId>
            <artifactId>TwentyThreeArt</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>javax</groupId>
            <artifactId>javaee-web-api</artifactId>
            <version>7.0</version>
            <scope>provided</scope>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.1</version>
                <configuration>
                    <source>1.7</source>
                    <target>1.7</target>
                    <compilerArguments>
                        <endorseddirs>${endorsed.dir}</endorseddirs>
                    </compilerArguments>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-war-plugin</artifactId>
                <version>2.3</version>
                <configuration>
                    <failOnMissingWebXml>false</failOnMissingWebXml>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <version>2.6</version>
                <executions>
                    <execution>
                        <phase>validate</phase>
                        <goals>
                            <goal>copy</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${endorsed.dir}</outputDirectory>
                            <silent>true</silent>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>javax</groupId>
                                    <artifactId>javaee-endorsed-api</artifactId>
                                    <version>7.0</version>
                                    <type>jar</type>
                                </artifactItem>
                            </artifactItems>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
    <repositories>
        <repository>
            <url>http://repository.primefaces.org/</url>
            <id>PrimeFaces-maven-lib</id>
            <layout>default</layout>
            <name>Repository for library PrimeFaces-maven-lib</name>
        </repository>
    </repositories>
</project>
